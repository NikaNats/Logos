[run]
branch = False
source =
    .

omit =
    .venv/*
    */site-packages/*
    packages/logos-vscode/src/*
    packages/logos-liturgy/*
    */bin/*
    */dist/*
    */out/*
    */node_modules/*
    */package-lock.json
    */package.json
    */*.tmLanguage.json
    tests/*

[report]
fail_under = 100
show_missing = True
skip_empty = True

include =
    logos.py

exclude_lines =
    pragma: no cover
    if __name__ == .__main__.: 
