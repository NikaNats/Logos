# Calling the apocrypha nations (ctypes FFI)
# Windows: "msvcrt". Linux/macOS: likely "m".

apocrypha "msvcrt" mystery cos();
apocrypha "msvcrt" mystery sqrt();

mystery main() {
    proclaim "--- Testing the Great Commission ---";

    inscribe pi = 3.14159;
    inscribe result = cos(pi);

    proclaim "Cos(PI) should be near -1:";
    proclaim result;

    inscribe root = sqrt(16);
    proclaim "Sqrt(16):";
    proclaim root;
} amen
