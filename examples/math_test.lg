mystery main() {
    inscribe a : HolyInt = 0 - 50;
    inscribe b : HolyInt = 20;

    inscribe dist : HolyInt = abs(a);
    inscribe highest : HolyInt = max(dist, b);

    say("The absolute value is:");
    say_int(dist);

    say("The highest value is:");
    say_int(highest);

    say("Chanting:");
    say(chant("Amen!", 3));
} amen
