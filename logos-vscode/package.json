{
    "name": "logos-lang",
    "displayName": "Logos (Orthodox)",
    "description": "Liturgical programming language support for Logos (.lg)",
    "version": "1.0.0",
    "publisher": "Theophilus",
    "engines": {
        "vscode": "^1.75.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "logos",
            "aliases": ["Logos", "logos"],
            "extensions": [".lg"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "logos",
            "scopeName": "source.logos",
            "path": "./syntaxes/logos.tmLanguage.json"
        }]
    }
}
