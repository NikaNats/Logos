tradition "lib/genesis.lg";

mystery main() -> Void {
    proclaim "--- Standard Library Smoke Test ---";

    inscribe t0: HolyInt = now();
    proclaim t0;

    inscribe fd = open_scroll("monastery.log", 1);
    write_line(fd, "Service started");
    close_scroll(fd);

    inscribe t1: HolyInt = now();
    proclaim (t1 - t0);
} amen
